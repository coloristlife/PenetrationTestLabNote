jQuery(function($){if(Device.isScreen()){$('.left-sticky').stick_in_parent();}
if($('body').hasClass('single')){$(document).on('click','.cboxElement',function(e){var href=$(this).attr('href');if(typeof href!=='undefined'&&href.length&&href.indexOf('/go/')>=0){window.location.assign(href);}});var timerId=setInterval(function(){var found=false;$('.cboxElement').each(function(){var href=$(this).attr('href');if(typeof href!=='undefined'&&href.length&&href.indexOf('/go/')>=0){$(this).removeClass('cboxElement');found=true;}});if(found){clearInterval(timerId);}},1000);}
$(document).ajaxComplete(function(){$(document.body).trigger("sticky_kit:recalc");});});